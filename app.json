{
  "expo": {
    "name": "Music_App_GroupProject",
    "slug": "Music_App_GroupProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/texture/LPAMB.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/texture/LPAMB.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/texture/LPAMB.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Caudex.ttf",
            "./assets/fonts/Consola.ttf"
          ]
        }
      ],
      "expo-asset"
    ]
  }
}
