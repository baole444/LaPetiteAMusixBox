{
  "expo": {
    "name": "La Petite A Musix Box",
    "slug": "a_petite_a_musix_box",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/texture/LPAMB.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "arsky.LPAMB.player",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "arsky.LPAMB.player",
      "adaptiveIcon": {
        "foregroundImage": "./assets/texture/LPAMB.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/texture/LPAMB.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Caudex.ttf",
            "./assets/fonts/Consola.ttf"
          ]
        }
      ],
      "expo-asset",
      "expo-secure-store",
      "expo-build-properties"
    ],
    "extra": {
      "eas": {
        "projectId": "d6ff398d-7350-4a7e-b9d7-b3c579bcbf21"
      }
    }
  }
}
